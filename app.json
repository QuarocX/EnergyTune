{
  "expo": {
    "name": "EnergyTune",
    "slug": "energytune",
    "scheme": "energytune",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quarocx.energytune",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "POST_NOTIFICATIONS",
        "POST_NOTIFICATIONS"
      ],
      "package": "com.quarocx.energytune"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f8dd1d4d-af5a-41aa-bd2a-b1db8d352636"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "color": "#007AFF",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#007AFF",
      "androidMode": "default",
      "androidCollapsedTitle": "Check in with EnergyTune"
    }
  }
}
